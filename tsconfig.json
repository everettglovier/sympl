{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "sympl-chat": [
        "dist/sympl-chat"
      ],
      "sympl-chat/*": [
        "dist/sympl-chat/*"
      ],
      "sympl-context": [
        "dist/sympl-context"
      ],
      "sympl-context/*": [
        "dist/sympl-context/*"
      ],
      "sympl-modal": [
        "dist/sympl-modal"
      ],
      "sympl-modal/*": [
        "dist/sympl-modal/*"
      ],
      "sympl-form": [
        "dist/sympl-form"
      ],
      "sympl-form/*": [
        "dist/sympl-form/*"
      ],
      "sympl-spinner": [
        "dist/sympl-spinner"
      ],
      "sympl-spinner/*": [
        "dist/sympl-spinner/*"
      ],
      "sympl-toggle": [
        "dist/sympl-toggle"
      ],
      "sympl-toggle/*": [
        "dist/sympl-toggle/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}